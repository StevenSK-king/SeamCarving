    <Version>1.0.0</Version>
    <AssemblyName>Zavolokas.SeamCarving</AssemblyName>
    <RootNamespace>Zavolokas.SeamCarving</RootNamespace>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
